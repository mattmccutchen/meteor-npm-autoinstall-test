{
  "name": "npm-autoinstall-test",
  "private": true,
  "scripts": {
    "start": "meteor run"
  },
  "dependencies": {
    "meteor-node-stubs": "~0.2.0",
    "babel-runtime": "6.18.0",
    "codemirror": "5.18.2"
  },
  "meteor": {
    "npmAutoInstall": {
       "command": "unset TOOL_NODE_FLAGS && meteor npm prune && meteor npm install",
       "watchSpec": ["package.json", "npm-shrinkwrap.json"]
    }
  }
}
